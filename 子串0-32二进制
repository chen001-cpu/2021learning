#include<stdio.h>
int main()
{
	int a,b,c,d,e;
	int i;
	a=0,b=0,c=0,d=0,e=0;
	for(i=0;i<32;i++)
	{
		a=i%2;
		b=i/2%2;
		c=i/4%2;
		d=i/8%2;
		e=i/16%2;
		printf("%d%d%d%d%d\n",e,d,c,b,a);
		
	}
	return 0;
}




#include<stdio.h>
int main()
{
	int t,h,m,s;
	scanf("%d",&t);
	s=t%60;
	m=t/60;
	h=m/60;
	if(h>0)
	m=m%60;
	printf("%d:%d:%d",h,m,s);
	return 0;
 } 
